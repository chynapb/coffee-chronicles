{
  "expo": {
    "name": "coffee-chronicles",
    "slug": "coffee-chronicles",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/coffee-icon.png",
    "scheme": "com.chynablair.coffeechronicles",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/coffee-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/icons/coffee-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptiveIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chynablair.coffeechronicles"
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "06b20348-348e-4273-9837-eeee01b200c9"
      }
    },
    "plugins": ["expo-router"]
  }
}
